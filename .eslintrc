{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": ["error", "unix"],
    "no-underscore-dangle": ["error", { "allow": ["_id", "__dirname"] }],
    "import/no-unresolved": "off",
    "import/extensions": ["error", "ignorePackages"]
  }
}
